msgstr "목표대상(target) 플랫폼으로 제작 중: %s\n"

#: build.c:321
#, c-format
msgid "Building for target %s\n"
msgstr "%s (을)를 제작하고 있습니다\n"

#: rpm.c:227
#, c-format
msgid "rpm: %s\n"
msgstr "rpm: %s\n"

#: rpm.c:238 rpmqv.c:224
#, c-format
msgid "RPM version %s\n"
msgstr "RPM - %s\n"

#: rpm.c:242 rpmqv.c:230
